{
	"name": "sqlite-wiki-search-worker",
	"version": "1.0.0",
	"description": "Public search interface for SQLite MCP Server Wiki using Cloudflare AutoRAG",
	"main": "src/index.ts",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy --env=\"\"",
		"deploy:production": "wrangler deploy --env production",
		"tail": "wrangler tail",
		"lint": "eslint src --ext .ts",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"cloudflare",
		"workers",
		"autorag",
		"sqlite",
		"mcp",
		"search"
	],
	"author": "Adamic",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20260111.0",
		"@eslint/js": "^9.18.0",
		"eslint": "^9.18.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.52.0",
		"wrangler": "^4.58.0"
	},
	"overrides": {
		"esbuild": "^0.25.10"
	}
}